<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>oralhealth2</id>
	<name>Oral Health Assessment Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.oralhealth2</package>
	<author>Anja</author>
	<description>
		module for an Oral Health Assessment
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/oralhealth2/update.rdf</updateURL>
	<require_version>1.9.3</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.oralhealth2.oralhealthmoduleActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.oralhealth2.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		oralhealthmodule.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with oralhealth2. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

